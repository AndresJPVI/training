<!DOCTYPE html>
<html class="MoviePage" lang="{{#with language}}{{this}}{{else}}en{{/with}}" itemscope itemtype="http://schema.org/WebPage">
    {{include "/core/page/Page-head.hbs" this}}
    <body class="MoviePage-body" {{~#each extraBodyAttributes}} {{this}} {{~/each~}}
        {{#if hidePromoDropShadow}} data-hide-promo-dropshadow{{/if}}
        {{#if promoModuleAlignment}} data-promo-module-alignment="{{promoModuleAlignment}}"{{/if}}
        {{#if boldMood}} data-mood-bold{{/if}}
        {{#if inlineSubheading}} data-subheading-inline="{{inlineSubheading}}"{{/if}}
        {{#if hideExternalLinkIcon}}data-hide-external-link-icon{{/if}}
        {{#if hat}}data-show-hat{{/if}}
        {{#if hideHeaderHat}}data-hide-header-hat{{/if}}
    >
    {{include "/core/page/Page-icons.hbs" this}}
    {{#with hat}}
        <div class="MoviePage-hat">{{this}}</div>
    {{/with}}
    <header class="MoviePage-header" data-with-toggle-menu itemscope itemtype="http://schema.org/WPHeader"
        {{#if mobileLeftAlignNavigation}}data-mobile-left-align-nav{{/if}}>
        <div class="MoviePage-header-contentWrapper">
            {{#with logo}}
                <div class="MoviePage-logo">
                    {{this}}
                </div>
            {{/with}}
            {{#with navigation}}
                <psd-toggler
                    class="MoviePage-navigation-menuToggle"
                    aria-role="button"
                    aria-label="Menu"
                    aria-controls="navigation"
                    toggle-trigger="navigation">
                    <svg class="burger-menu"><use xlink:href="#burger-menu"/></svg>
                    <svg class="close-x"><use xlink:href="#close-x"/></svg>
                </psd-toggler>
                <div class="MoviePage-navigation" data-main-navigation data-toggle-item="navigation"{{#if ../navigationAlignment}}data-navigation-alignment="{{../navigationAlignment}}"{{/if}}>
                    {{this}}
                </div>
            {{/with}}
            {{#if searchAction}}
                <form class="Header-search" action="{{searchAction}}" novalidate="" autocomplete="off" data-header-search>
                    <input type="text" class="MoviePage-searchInput" name="q" placeholder="{{format "/core/page/Page" "placeholderLabel"}}">
                    {{include "/core/page/Page-searchButton.hbs" this}}
                </form>
            {{/if}}
        </div>
    </header>
    <div class="MoviePage-contentWrapper"
        data-negative-margin={{#if negativeMargin}}"{{negativeMargin}}"{{else}}default{{/if}}>
        {{include "/core/page/Page-sectionNavigation.hbs" this}}
        {{#with above}}
            <div class="MoviePage-above">{{this}}</div>
        {{/with}}
        <main class="MoviePage-main">
            {{#with breadcrumbs}}
                <div class="MoviePage-breadcrumbs">{{this}}</div>
            {{/with}}
            <div class="MoviePage-mainContent" itemscope itemtype="http://schema.org/Person"{{extraAttributes}}>
                <div class="MoviePage-info">
                    <div class="MoviePage-info-card">
                        {{#with cover}}
                            <div class="MoviePage-cover" itemprop="image">{{this}}</div>
                        {{/with}}
                        <div class="MoviePage-info-data">
                            {{#with name}}
                                <h1 class="MoviePage-name" itemprop="name">{{this}}</h1>
                            {{/with}}
                            {{#with phase}}
                                <div class="MoviePage-phase">{{this}}</div>
                            {{/with}}
                            {{#with releaseDate}}
                                <div class="MoviePage-releaseDate">{{this}}</div>
                            {{/with}}
                            {{#with summary}}
                                <div class="MoviePage-summary">{{this}}</div>
                            {{/with}}
                        </div>
                    </div>
                    <hr>
                        {{#with plot}}
                            <h2 class="MoviePage-plot-header">Plot</h2>
                            <div class="MoviePage-plot" itemprop="description">{{this}}</div>
                        {{/with}}
                        <hr>
                        </div>
                        {{#with featuredCharacters}}
                            <div class="MoviePage-featuredCharacters">
                                {{render this [/core/list/List.hbs]="/core/list/ListFourColumn.hbs"}}
                            </div>
                        {{/with}}
                    </div>
                    {{#with tags}}
                        <div class="MoviePage-tags">
                            <h2 class="MoviePage-tags-heading">{{format "/core/page/ContentPage" "tagsHeading"}}</h2>
                            <div class="MoviePage-tags-body">{{this}}</div>
                        </div>
                    {{/with}}
                    {{#with countries}}
                        <div class="MoviePage-countries">
                            <h2 class="MoviePage-countries-heading">{{format "/core/page/ContentPage" "countriesHeading"}}</h2>
                            <div class="MoviePage-countries-body">{{this}}</div>
                        </div>
                    {{/with}}
                    {{#with brands}}
                        <div class="MoviePage-brands">
                            <h2 class="MoviePage-brands-heading">{{format "/core/page/ContentPage" "brandsHeading"}}</h2>
                            <div class="MoviePage-brands-body">{{this}}</div>
                        </div>
                    {{/with}}
                    {{#with products}}
                        <div class="MoviePage-products">
                            <h2 class="MoviePage-products-heading">{{format "/core/page/ContentPage" "productsHeading"}}</h2>
                            <div class="MoviePage-products-body">{{this}}</div>
                        </div>
                    {{/with}}
                    {{#with topics}}
                        <div class="MoviePage-topics">
                            <h2 class="MoviePage-topics-heading">{{format "/core/page/ContentPage" "topicsHeading"}}</h2>
                            <div class="MoviePage-topics-body">{{this}}</div>
                        </div>
                    {{/with}}
                    {{#with actions}}
                        <div class="MoviePage-actions">
                            <h2 class="MoviePage-actions-heading">{{format "/core/page/ContentPage" "actionsHeading"}}</h2>
                            <div class="MoviePage-actions-body">{{this}}</div>
                        </div>
                    {{/with}}
                    {{#with comments}}
                        <div class="MoviePage-comments">
                            {{this}}
                        </div>
                    {{/with}}
                </main>
                {{#with below}}
                    <div class="MoviePage-below">{{this}}</div>
                {{/with}}
            </div>
            {{include "/core/page/Page-footer.hbs" this}}
            {{include "/core/page/Page-SearchOverlay.hbs" this}}
            {{include "/core/page/Page-extraBodyItems.hbs" this}}
        </body>
    </html>
