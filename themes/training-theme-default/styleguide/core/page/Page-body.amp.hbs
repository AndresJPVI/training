<body class="Page-body" {{~#each extraBodyAttributes}} {{this}} {{~/each~}}>
    {{include "/core/page/Page-header.amp.hbs" this}}
    {{include "/core/page/Page-banner.amp.hbs" this}}
    {{include "/core/page/Page-sectionNavigation.amp.hbs" this}}
    {{include "/core/page/Page-above.amp.hbs" this}}

    <main class="Page-main">
        {{#with pageHeading}}
            <h1 class="Page-pageHeading">{{this}}</h1>
        {{/with}}
        {{#with pageSubHeading}}
            <div class="Page-pageSubHeading">{{this}}</div>
        {{/with}}
        {{#with pageLead}}
            <div class="Page-pageLead">{{this}}</div>
        {{/with}}

        {{#with main}}{{this}}{{/with}}

        {{#with actions}}
            <div class="Page-actions">
                <h2 class="Page-actions-heading">{{format "/core/page/Page" "actionsHeading"}}</h2>
                <div class="Page-actions-body">{{this}}</div>
            </div>
        {{/with}}
    </main>

    {{include "/core/page/Page-aside.amp.hbs" this}}
    {{include "/core/page/Page-below.amp.hbs" this}}
    {{include "/core/page/Page-footer.amp.hbs" this}}
    {{include "/core/page/Page-extraBodyItems.amp.hbs" this}}
</body>
