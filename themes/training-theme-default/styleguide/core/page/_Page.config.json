{
  "styles": {
    "/core/page/Page.hbs": {
      "templates": [
        {
          "displayName": "Section Page",
          "example": "/examples/Section-Default.json"
        },
        {
          "displayName": "Home Page",
          "example": "/examples/HomePage.json"
        },
        {
          "displayName": "Error Page",
          "example": "/examples/ErrorPage.json",
          "fields": {
            "title": {
              "type": "text",
              "cms.ui.note": "Set a title"
