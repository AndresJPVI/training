{{#if items}}
    <ps-list-loadmore class="ListE"{{include "/core/list/List-attributes.hbs" this}}>
        {{include "/core/list/List-header.hbs" this className="ListE"}}

        {{#with items}}
            <ul class="ListE-items" data-list-loadmore-items>
                {{#each this}}
                <li class="ListE-items-item">
                    {{include "/core/promo/PromoB.hbs" this description=null}}
                </li>
                {{/each}}
            </ul>
        {{/with}}

        {{#with nextPage}}
            <div class="ListE-nextPage" data-list-loadmore-pagination>
                <a class="Link" href="{{this}}" rel="nofollow">{{format "/core/list/List" "loadMoreText"}}</a>
            </div>
        {{/with}}
    </ps-list-loadmore>
{{else}}{{~noResultsModule~}}{{/if}}