<div class="List-Amp" {{extraAttributes}}>
    {{include "/core/list/List-header.amp.hbs" this className="List-Amp"}}

    {{#with items}}
        <ul class="List-Amp-items">
            {{#each this}}
            <li class="List-Amp-items-item">
                {{include "/core/promo/PromoLink.amp.hbs" this}}
            </li>
            {{/each}}
        </ul>
    {{/with}}
</div>
