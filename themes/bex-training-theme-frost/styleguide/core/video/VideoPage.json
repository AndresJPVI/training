        },
        {
            "_include": "/core/video/PlaylistItem.json",
            "url": "../video/VideoWatch2.html"
        }
    ],
    "companions": [
        {
          "_include": "CompanionContentItem.json"
        },
        {
          "_include": "CompanionContentItem.json"
        },
        {
          "_include": "CompanionContentItem.json"
        },
        {
          "_include": "CompanionContentItem.json"
        }
      ]
}
