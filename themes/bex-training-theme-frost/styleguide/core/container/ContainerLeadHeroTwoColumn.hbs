<div class="TwoColumnContainer">
    {{#with columnOne}}
        <div class="TwoColumnContainer-columnOne">
            {{#each this}}
                {{render this [/core/list/List.hbs]="/core/list/ListChevron.hbs" [/core/promo/Promo.hbs]="/core/promo/PromoStory.hbs"}}
            {{/each}}
        </div>
    {{/with}}
    {{#with columnTwo}}
        <div class="TwoColumnContainer-columnTwo">
            {{#each this}}
                {{render this [/core/promo/Promo.hbs]=(cycle "/core/promo/PromoImageOnTop-large.hbs" "/core/promo/PromoImageOnTop.hbs" repeatLast=1)}}
            {{/each}}
        </div>
    {{/with}}
</div>
