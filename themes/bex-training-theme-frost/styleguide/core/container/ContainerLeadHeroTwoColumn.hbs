{{#block "TwoColumnContainer" extend="/core/container/TwoColumnContainer.hbs"}}
    <div class="{{blockName}}">
        {{#element "columnOne"}}
            <div class="{{elementName}}">
                {{#each this}}
                    {{render this
                        [/core/list/List.hbs]="/core/list/ListChevron.hbs"
                        [/core/promo/Promo.hbs]="/core/promo/PromoStory.hbs"
                    }}
                {{/each}}
            </div>
        {{/element}}
        {{#element "columnTwo"}}
            <div class="{{elementName}}">
                {{#each this}}
                {{render this
                    [/core/promo/Promo.hbs]=(cycle
                                        "/core/promo/PromoImageOnTop-large.hbs"
                                        "/core/promo/PromoImageOnTop.hbs"
                                        repeatLast=1
                                )
                }}
                {{/each}}
            </div>
        {{/element}}
    </div>
{{/block}}
