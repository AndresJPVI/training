<div class="TwoColumnContainer">
    {{#with columnOne}}
        <div class="TwoColumnContainer-columnOne">
            {{#each this}}
                {{render this [/core/promo/Promo.hbs]="/core/promo/PromoImageOnTop-large.hbs"}}
            {{/each}}
        </div>
    {{/with}}
    {{#with columnTwo}}
        <div class="TwoColumnContainer-columnTwo">
            {{#each this}}
                {{render this [/core/list/List.hbs]="/core/list/ListHorizontalRule.hbs"}}
            {{/each}}
        </div>
    {{/with}}
</div>
