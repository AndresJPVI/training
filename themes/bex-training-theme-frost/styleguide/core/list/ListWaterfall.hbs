<div class="ListWaterfall"{{extraAttributes}}>
    {{#with items}}
        <div class="ListWaterfall-items">
            <div class="grid-sizer"></div>
            <div class="gutter-sizer"></div>
            {{#each this}}
                <div class="grid-item">
                    {{render this [/core/promo/Promo.hbs]="/core/promo/PromoImageOnTop.hbs"}}
                </div>
            {{/each}}
        </div>
    {{/with}}
</div>
