{{#block "ListFourColumnNarrow" override="ListGrid.hbs"}}
    {{#element "items"}}
        <div class="{{elementName}}">
            {{#partition this size=4}}
                <div class="{{blockName}}-row">
                    {{#each this}}
                        <div class="{{elementName}}-item">
                            {{render this [/core/promo/Promo.hbs]="/core/promo/PromoImage.hbs"}}
                        </div>
                    {{/each}}
                </div>
            {{/partition}}
        </div>
    {{/element}}
{{/block}}
