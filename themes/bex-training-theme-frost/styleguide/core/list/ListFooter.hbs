<div class="ListFooter">
    {{#with title}}
        <div class="ListFooter-title">{{this}}</div>
    {{/with}}
    {{#with items}}
        <ul class="ListFooter-items">
            {{#each this}}
                <li class="ListFooter-items-item">
                    {{render this [/core/promo/Promo.hbs]="/core/promo/PromoTextItem.hbs"}}
                </li>
            {{/each}}
        </ul>
    {{/with}}
</div>
