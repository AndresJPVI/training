<App>
    <screens>{{screens}}</screens>
    <styleOverride>
        <AppStyle style="theme">
            <colorPrimary>{{colorPrimary}}</colorPrimary>
            <colorBackground>{{colorBackground}}</colorBackground>
            <colorHeaderBackground>{{colorHeaderBackground}}</colorHeaderBackground>
            <colorFont>{{colorFont}}</colorFont>
            <colorHeaderFont>{{colorHeaderFont}}</colorHeaderFont>
            <colorDescription>{{colorDescription}}</colorDescription>
            <colorNote>{{colorNote}}</colorNote>
            <colorBorder>{{colorBorder}}</colorBorder>
            <colorLightGray>{{colorLightGray}}</colorLightGray>
            <colorDarkGray>{{colorDarkGray}}</colorDarkGray>
            <colorWhite>{{colorWhite}}</colorWhite>
            <colorBlack>{{colorBlack}}</colorBlack>
            <borderWidth_thin>{{borderWidth_thin}}</borderWidth_thin>
            <borderWidth_thick>{{borderWidth_thick}}</borderWidth_thick>
            <fontSize_xs>{{fontSize_xs}}</fontSize_xs>
            <fontSize_s>{{fontSize_s}}</fontSize_s>
            <fontSize_m>{{fontSize_m}}</fontSize_m>
            <fontSize_l>{{fontSize_l}}</fontSize_l>
            <fontSize_xl>{{fontSize_xl}}</fontSize_xl>
            <fontSize_xxl>{{fontSize_xxl}}</fontSize_xxl>
            <gap_xs>{{gap_xs}}</gap_xs>
            <gap_s>{{gap_s}}</gap_s>
            <gap_m>{{gap_m}}</gap_m>
            <gap_l>{{gap_l}}</gap_l>
            <gap_xl>{{gap_xl}}</gap_xl>
            <iconSize_xs>{{iconSize_xs}}</iconSize_xs>
            <iconSize_s>{{iconSize_s}}</iconSize_s>
            <iconSize_m>{{iconSize_m}}</iconSize_m>
            <iconSize_l>{{iconSize_l}}</iconSize_l>
            <iconSize_xl>{{iconSize_xl}}</iconSize_xl>
            <lineHeight_xs>{{lineHeight_xs}}</lineHeight_xs>
            <lineHeight_s>{{lineHeight_s}}</lineHeight_s>
            <lineHeight_m>{{lineHeight_m}}</lineHeight_m>
            <lineHeight_l>{{lineHeight_l}}</lineHeight_l>
            <lineHeight_xl>{{lineHeight_xl}}</lineHeight_xl>
            <lineHeight_xxl>{{lineHeight_xxl}}</lineHeight_xxl>
        </AppStyle>
    </styleOverride>
</App>
