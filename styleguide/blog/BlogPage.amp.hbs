<!DOCTYPE html>
<html âš¡ class="BlogPage" lang="{{#with language}}{{this}}{{else}}en{{/with}}" itemscope itemtype="http://schema.org/WebPage">
    <head>
        {{include "/core/page/Page-charset.hbs" this}}
        {{include "/core/page/Page-webFontLink.amp.hbs" this}}
        {{include "/core/page/Page-styles.amp.hbs" this}}
        {{include "/core/page/Page-jsonLinkedData.hbs" this}}
        <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
        {{include "/core/page/Page-title.hbs" this}}
        {{include "/core/page/Page-description.hbs" this}}
        {{include "/core/page/Page-keywords.hbs" this}}
        {{include "/core/page/Page-canonicalLink.hbs" this}}
        {{include "/core/page/Page-meta.hbs" this}}
        {{include "/core/page/Page-favicons.hbs" this}}
        {{include "/core/page/Page-manifestLink.hbs" this}}
        <meta name="viewport" content="width=device-width, minimum-scale=1, initial-scale=1">
        <script async src="https://cdn.ampproject.org/v0.js"></script>
        {{include "/core/page/Page-ampJs.amp.hbs" this}}
        {{#if searchAction}}
            <script async custom-element="amp-form" src="https://cdn.ampproject.org/v0/amp-form-0.1.js"></script>
        {{/if}}
    </head>
    <body class="BlogPage-body"{{~#each extraBodyAttributes}} {{this}}{{~/each~}}>
        <header class="BlogPage-header" itemscope itemtype="http://schema.org/WPHeader">
            {{#with hat}}
                <div class="BlogPage-hat">{{this}}</div>
            {{/with}}
            {{include "/core/page/Page-logo.amp.hbs" this}}
            {{include "/core/page/Page-navigation.amp.hbs" this}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#if searchAction}}
                <form class="BlogPage-search" action="{{searchAction}}" novalidate="" autocomplete="off">
                    <input type="text" class="BlogPage-searchInput" name="q">
                    <button type="submit" class="BlogPage-searchButton">{{format "/core/page/Page" "searchButtonLabel"}}</button>
                </form>
            {{/if}}
        </header>
        {{#with banner}}
            <div class="BlogPage-banner">{{~this~}}</div>
        {{/with}}
        {{#with sectionNavigation}}
            <div class="BlogPage-sectionNavigation">{{this}}</div>
        {{/with}}
        {{#with packageNavigation}}
            <div class="BlogPage-packageNavigation">{{this}}</div>
        {{/with}}
        {{#with above}}
            <div class="BlogPage-above">{{this}}</div>
        {{/with}}
        {{#if showPageHeading}}
            {{#with pageHeading}}
                <div class="BlogPage-pageHeading">{{this}}</div>
            {{/with}}
        {{/if}}
        <main class="BlogPage-main">
            {{#with breadcrumbs}}
                <div class="BlogPage-breadcrumbs">{{this}}</div>
            {{/with}}
            {{#with blogNavigation}}
                <div class="BlogPage-BlogPage-blogNavigation">{{this}}</div>
            {{/with}}
            <article class="BlogPage">
                {{#with blogLead}}
                    <div class="BlogPage-blogLead">{{this}}</div>
                {{/with}}
                {{#with posts}}
                    <section class="BlogPage-posts">
                        {{~#each this~}}
                            {{this}}
                        {{~/each~}}
                    </section>
                {{/with}}
                <nav>
                    <ul class="BlogPage-pagination">
                        {{#with previousPage}}
                            <li class="BlogPage-previousPage"><a href="{{this}}" rel="prev">{{format "/blog/BlogPage" "previousPage"}}</a></li>
                        {{/with}}
                        <li class="BlogPage-pageCounts">{{format "/blog/BlogPage" "pageCounts" number=pageNumber count=pageCount}}</li>
                        {{#with nextPage}}
                            <li class="BlogPage-nextPage"><a href="{{this}}" rel="next">{{format "/blog/BlogPage" "nextPage"}}</a></li>
                        {{/with}}
                    </ul>
                </nav>
            </article>
            {{#with tags}}
                <div class="BlogPage-tags">
                    <h2 class="BlogPage-tags-heading">{{format "/core/page/ContentPage" "tagsHeading"}}</h2>
                    <div class="BlogPage-tags-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with countries}}
                <div class="BlogPage-countries">
                    <h2 class="BlogPage-countries-heading">{{format "/core/page/ContentPage" "countriesHeading"}}</h2>
                    <div class="BlogPage-countries-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with brands}}
                <div class="BlogPage-brands">
                    <h2 class="BlogPage-brands-heading">{{format "/core/page/ContentPage" "brandsHeading"}}</h2>
                    <div class="BlogPage-brands-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with products}}
                <div class="BlogPage-products">
                    <h2 class="BlogPage-products-heading">{{format "/core/page/ContentPage" "productsHeading"}}</h2>
                    <div class="BlogPage-products-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with topics}}
                <div class="BlogPage-topics">
                    <h2 class="BlogPage-topics-heading">{{format "/core/page/ContentPage" "topicsHeading"}}</h2>
                    <div class="BlogPage-topics-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with actions}}
                <div class="BlogPage-actions">
                    <h2 class="BlogPage-actions-heading">{{format "/core/page/ContentPage" "actionsHeading"}}</h2>
                    <div class="BlogPage-actions-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with comments}}
                <div class="BlogPage-comments">
                    {{this}}
                </div>
            {{/with}}
        </main>
        {{#with aside}}
            <aside class="BlogPage-aside">{{this}}</aside>
        {{/with}}
        {{#with below}}
            <div class="BlogPage-below">{{this}}</div>
        {{/with}}
        <footer class="BlogPage-footer" itemscope itemtype="https://schema.org/WPFooter">
            {{include "/core/page/Page-logo.amp.hbs" this}}
            {{include "/core/page/Page-navigation.amp.hbs" this}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#with disclaimer}}
                <div class="BlogPage-disclaimer">{{this}}</div>
            {{/with}}
        </footer>
        {{include "/core/page/Page-ampIntegrations.amp.hbs" this}}
        {{include "/core/page/Page-ampAnalytics.amp.hbs" this}}
    </body>
</html>
