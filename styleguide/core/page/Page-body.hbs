<body class="Page-body"{{~#each extraBodyAttributes}} {{this}}{{~/each~}}>
    <header class="Page-header" itemscope itemtype="http://schema.org/WPHeader">
        {{#with hat}}
            <div class="Page-hat">{{this}}</div>
        {{/with}}
        {{#with logo}}
            <div class="Page-logo">{{this}}</div>
        {{/with}}
        {{#with navigation}}
            <div class="Page-navigation">{{this}}</div>
        {{/with}}
        {{#with social}}
            <div class="Page-social">{{this}}</div>
        {{/with}}
        {{#if searchAction}}
            <form class="Page-search" action="{{searchAction}}" novalidate="" autocomplete="off">
                <input type="text" class="Page-searchInput" name="q">
                <button type="submit" class="Page-searchButton">{{format "/core/page/Page" "searchButtonLabel"}}</button>
            </form>
        {{/if}}
    </header>
    {{#with banner}}
        <div class="Page-banner">{{~this~}}</div>
    {{/with}}
    {{#with sectionNavigation}}
        <div class="Page-sectionNavigation">{{this}}</div>
    {{/with}}
    {{#with packageNavigation}}
        <div class="Page-packageNavigation">{{this}}</div>
    {{/with}}
    {{#with above}}
        <div class="Page-above">{{this}}</div>
    {{/with}}
    <main class="Page-main">
        {{#with pageHeading}}
            <h1 class="Page-pageHeading">{{this}}</h1>
        {{/with}}
        {{#with pageSubHeading}}
            <div class="Page-pageSubHeading">{{this}}</div>
        {{/with}}
        {{#with pageLead}}
            <div class="Page-pageLead">{{this}}</div>
        {{/with}}
        {{#with main}}{{this}}{{/with}}
        {{#with actions}}
            <div class="Page-actions">
                <h2 class="Page-actions-heading">{{format "/core/page/Page" "actionsHeading"}}</h2>
                <div class="Page-actions-body">{{this}}</div>
            </div>
        {{/with}}
    </main>
    {{#with aside}}
        <aside class="Page-aside">{{this}}</aside>
    {{/with}}
    {{#with below}}
        <div class="Page-below">{{this}}</div>
    {{/with}}
    <footer class="Page-footer" itemscope itemtype="https://schema.org/WPFooter">
        {{#with footerLogo}}
            <div class="Page-footerLogo">{{this}}</div>
        {{/with}}
        {{#with footerNavigation}}
            <div class="Page-footerNavigation">{{this}}</div>
        {{/with}}
        {{#with footerContent}}
            <div class="Page-footerContent">{{this}}</div>
        {{/with}}
        {{#with social}}
            <div class="Page-social">{{this}}</div>
        {{/with}}
        {{#with disclaimer}}
            <div class="Page-disclaimer">{{this}}</div>
        {{/with}}
    </footer>
    {{include "/core/page/Page-extraBodyItems.hbs" this}}
</body>
