{{#block "PageLogo"}}
    {{#element "image" noWith=true}}
        {{#if href~}}
            <a href="{{href}}"{{#with target}} target="{{this}}"{{/with}}{{extraAttributes}}>
        {{~/if}}
                <img class="{{elementName}}" src="{{image.src}}" alt="{{alt}}"
                    {{~#with image.srcset}} srcset="{{this}}"{{/with~}}
                    {{~#with image.width}} width="{{this}}"{{/with~}}
                    {{~#with image.height}} height="{{this}}"{{/with~}} />
        {{#if href~}}
            </a>
        {{~/if}}
    {{/element}}
{{/block}}
