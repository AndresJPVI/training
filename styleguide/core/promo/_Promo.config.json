{
  "styles": {
    "/core/promo/Promo.native.hbs": {
      "example": "/core/promo/Promo.json",
      "variations": [
        {
          "displayName": "Promo - Image Left",
          "template": "/core/promo/PromoImageLeft.native.hbs"
        },
        {
          "displayName": "Promo - Image Top",
          "template": "/core/promo/PromoImageTop.native.hbs"
        },
        {
          "displayName": "Promo - Image Background",
          "template": "/core/promo/PromoImageBackground.native.hbs"
        },
        {
          "displayName": "Promo - Author Promo",
          "hidden": true,
          "template": "/core/promo/AuthorPromo.native.hbs"
        }
      ]
    }
  },
  "imageSizes": {
