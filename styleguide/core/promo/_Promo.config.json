          "image": "nativeSquare"
        }
      }
    },
    "/core/promo/PromoImageTop.native.hbs": {
      "media": {
        "/core/figure/Figure.native.hbs": {
          "image": "nativeLandscape"
        },
        "/core/image/Image.native.hbs": {
          "image": "nativeLandscape"
        }
      }
    },
    "/core/promo/PromoImageBackground.native.hbs": {
      "media": {
        "/core/figure/Figure.native.hbs": {
          "image": "nativeLandscape"
        },
        "/core/image/Image.native.hbs": {
          "image": "nativeLandscape"
        }
      }
    }
  },
  "_hidden": true
}