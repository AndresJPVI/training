<div class="Container">
    {{#with title}}
        <div class="Container-title">
            {{this}}
        </div>
    {{/with}}
    {{#with description}}
        <div class="Container-description">
            {{this}}
        </div>
    {{/with}}
    {{#with rows}}
        <ul class="Container-rows">
            {{#each this}}
                <li class="Container-rows-item">{{this}}</li>
            {{/each}}
        </ul>
    {{/with}}
</div>
