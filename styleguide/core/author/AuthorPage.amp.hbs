<!DOCTYPE html>
<html âš¡ class="AuthorPage" lang="{{#with language}}{{this}}{{else}}en{{/with}}" itemscope itemtype="http://schema.org/WebPage">
    <head>
        {{include "/core/page/Page-charset.hbs" this}}
        {{include "/core/page/Page-webFontLink.amp.hbs" this}}
        {{include "/core/page/Page-styles.amp.hbs" this}}
        {{include "/core/page/Page-jsonLinkedData.hbs" this}}
        <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
        {{include "/core/page/Page-title.hbs" this}}
        {{include "/core/page/Page-description.hbs" this}}
        {{include "/core/page/Page-keywords.hbs" this}}
        {{include "/core/page/Page-canonicalLink.hbs" this}}
        {{include "/core/page/Page-meta.hbs" this}}
        {{include "/core/page/Page-favicons.hbs" this}}
        {{include "/core/page/Page-manifestLink.hbs" this}}
        <meta name="viewport" content="width=device-width, minimum-scale=1, initial-scale=1">
        <script async src="https://cdn.ampproject.org/v0.js"></script>
        {{include "/core/page/Page-ampJs.amp.hbs" this}}
        {{#if searchAction}}
            <script async custom-element="amp-form" src="https://cdn.ampproject.org/v0/amp-form-0.1.js"></script>
        {{/if}}
    </head>
    <body class="AuthorPage-body"{{~#each extraBodyAttributes}} {{this}}{{~/each~}}>
        <header class="AuthorPage-header" itemscope itemtype="http://schema.org/WPHeader">
            {{#with hat}}
                <div class="AuthorPage-hat">{{this}}</div>
            {{/with}}
            {{include "/core/page/Page-logo.amp.hbs" this}}
            {{include "/core/page/Page-navigation.amp.hbs" this}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#if searchAction}}
                <form class="AuthorPage-search" action="{{searchAction}}" novalidate="" autocomplete="off">
                    <input type="text" class="AuthorPage-searchInput" name="q">
                    <button type="submit" class="AuthorPage-searchButton">{{format "/core/page/Page" "searchButtonLabel"}}</button>
                </form>
            {{/if}}
        </header>
        {{#with banner}}
            <div class="AuthorPage-banner">{{~this~}}</div>
        {{/with}}
        {{#with sectionNavigation}}
            <div class="AuthorPage-sectionNavigation">{{this}}</div>
        {{/with}}
        {{#with packageNavigation}}
            <div class="AuthorPage-packageNavigation">{{this}}</div>
        {{/with}}
        {{#with above}}
            <div class="AuthorPage-above">{{this}}</div>
        {{/with}}
        {{#if showPageHeading}}
            {{#with pageHeading}}
                <div class="AuthorPage-pageHeading">{{this}}</div>
            {{/with}}
        {{/if}}
        <main class="AuthorPage-main">
            {{#with breadcrumbs}}
                <div class="AuthorPage-breadcrumbs">{{this}}</div>
            {{/with}}
            <div class="AuthorPage-mainContent" itemscope itemtype="http://schema.org/Person"{{extraAttributes}}>
                {{#with name}}
                    <h1 class="AuthorPage-name" itemprop="name">{{this}}</h1>
                {{/with}}
                {{#with affiliation}}
                    <h2 class="AuthorPage-affiliation" itemprop="affiliation">{{this}}</h2>
                {{/with}}
                {{#with jobTitle}}
                    <div class="AuthorPage-jobTitle" itemprop="jobTitle">{{this}}</div>
                {{/with}}
                {{#with image}}
                    <div class="AuthorPage-image" itemprop="image">{{this}}</div>
                {{/with}}
                {{#with shortBiography}}
                    <div class="AuthorPage-shortBiography">{{this}}</div>
                {{/with}}
                {{#with fullBiography}}
                    <div class="AuthorPage-fullBiography" itemprop="description">{{this}}</div>
                {{/with}}
                {{#with links}}
                    <ul class="AuthorPage-links">
                        {{#each this}}
                            <li class="AuthorPage-links-item">{{this}}</li>
                        {{/each}}
                    </ul>
                {{/with}}
            </div>
            {{#with tags}}
                <div class="AuthorPage-tags">
                    <h2 class="AuthorPage-tags-heading">{{format "/core/page/ContentPage" "tagsHeading"}}</h2>
                    <div class="AuthorPage-tags-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with countries}}
                <div class="AuthorPage-countries">
                    <h2 class="AuthorPage-countries-heading">{{format "/core/page/ContentPage" "countriesHeading"}}</h2>
                    <div class="AuthorPage-countries-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with brands}}
                <div class="AuthorPage-brands">
                    <h2 class="AuthorPage-brands-heading">{{format "/core/page/ContentPage" "brandsHeading"}}</h2>
                    <div class="AuthorPage-brands-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with products}}
                <div class="AuthorPage-products">
                    <h2 class="AuthorPage-products-heading">{{format "/core/page/ContentPage" "productsHeading"}}</h2>
                    <div class="AuthorPage-products-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with topics}}
                <div class="AuthorPage-topics">
                    <h2 class="AuthorPage-topics-heading">{{format "/core/page/ContentPage" "topicsHeading"}}</h2>
                    <div class="AuthorPage-topics-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with actions}}
                <div class="AuthorPage-actions">
                    <h2 class="AuthorPage-actions-heading">{{format "/core/page/ContentPage" "actionsHeading"}}</h2>
                    <div class="AuthorPage-actions-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with comments}}
                <div class="AuthorPage-comments">
                    {{this}}
                </div>
            {{/with}}
        </main>
        {{#with aside}}
            <aside class="AuthorPage-aside">{{this}}</aside>
        {{/with}}
        {{#with below}}
            <div class="AuthorPage-below">{{this}}</div>
        {{/with}}
        <footer class="AuthorPage-footer" itemscope itemtype="https://schema.org/WPFooter">
            {{include "/core/page/Page-logo.amp.hbs" this}}
            {{include "/core/page/Page-navigation.amp.hbs" this}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#with disclaimer}}
                <div class="AuthorPage-disclaimer">{{this}}</div>
            {{/with}}
        </footer>
        {{include "/core/page/Page-ampIntegrations.amp.hbs" this}}
        {{include "/core/page/Page-ampAnalytics.amp.hbs" this}}
    </body>
</html>
