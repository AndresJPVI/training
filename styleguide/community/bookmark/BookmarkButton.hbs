<button class="BookmarkButton"
    {{~#with type}} data-type="{{this}}"{{/with~}}
    {{~#with state}} data-state="{{this}}"{{/with~}}
    {{~#with endpoint}}data-url="{{this}}"{{/with~}}
    {{~#with authenticationUrl}}data-auth-url="{{this}}"{{/with~}}
    {{~#with target}}data-target="{{this}}"{{/with~}}>
    {{include "/community/affinity/AffinityButton-count.amp.hbs" this}}
    {{include "/community/affinity/AffinityButton-icon.amp.hbs" this}}
    <span class="BookmarkButton-label">{{format "/community/bookmark/BookmarkButton" "buttonLabel"}}</span>
</button>
