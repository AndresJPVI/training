<!DOCTYPE html>
<html âš¡ class="ShowPage" lang="{{#with language}}{{this}}{{else}}en{{/with}}" itemscope itemtype="http://schema.org/WebPage">
    <head>
        {{include "/core/page/Page-charset.hbs" this}}
        {{include "/core/page/Page-webFontLink.amp.hbs" this}}
        {{include "/core/page/Page-styles.amp.hbs" this}}
        {{include "/core/page/Page-jsonLinkedData.hbs" this}}
        <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>
        {{include "/core/page/Page-title.hbs" this}}
        {{include "/core/page/Page-description.hbs" this}}
        {{include "/core/page/Page-keywords.hbs" this}}
        {{include "/core/page/Page-canonicalLink.hbs" this}}
        {{include "/core/page/Page-meta.hbs" this}}
        {{include "/core/page/Page-favicons.hbs" this}}
        {{include "/core/page/Page-manifestLink.hbs" this}}
        <meta name="viewport" content="width=device-width, minimum-scale=1, initial-scale=1">
        <script async src="https://cdn.ampproject.org/v0.js"></script>
        {{include "/core/page/Page-ampJs.amp.hbs" this}}
        {{#if searchAction}}
            <script async custom-element="amp-form" src="https://cdn.ampproject.org/v0/amp-form-0.1.js"></script>
        {{/if}}
    </head>
    <body class="ShowPage-body"{{~#each extraBodyAttributes}} {{this}}{{~/each~}}>
        <header class="ShowPage-header" itemscope itemtype="http://schema.org/WPHeader">
            {{#with hat}}
                <div class="ShowPage-hat">{{this}}</div>
            {{/with}}
            {{include "/core/page/Page-logo.amp.hbs" this}}
            {{include "/core/page/Page-navigation.amp.hbs" this}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#if searchAction}}
                <form class="ShowPage-search" action="{{searchAction}}" novalidate="" autocomplete="off">
                    <input type="text" class="ShowPage-searchInput" name="q">
                    <button type="submit" class="ShowPage-searchButton">{{format "/core/page/Page" "searchButtonLabel"}}</button>
                </form>
            {{/if}}
        </header>
        {{#with banner}}
            <div class="ShowPage-banner">{{~this~}}</div>
        {{/with}}
        {{#with sectionNavigation}}
            <div class="ShowPage-sectionNavigation">{{this}}</div>
        {{/with}}
        {{#with packageNavigation}}
            <div class="ShowPage-packageNavigation">{{this}}</div>
        {{/with}}
        {{#with above}}
            <div class="ShowPage-above">{{this}}</div>
        {{/with}}
        {{#if showPageHeading}}
            {{#with pageHeading}}
                <div class="ShowPage-pageHeading">{{this}}</div>
            {{/with}}
        {{/if}}
        <main class="ShowPage-main">
            {{#with breadcrumbs}}
                <div class="ShowPage-breadcrumbs">{{this}}</div>
            {{/with}}
            <article class="ShowPage-mainContent" itemprop="mainEntity" itemscope itemtype="https://schema.org/TVSeries">
                {{#with headline}}
                    <h1 class="ShowPage-headline" itemprop="headline">{{this}}</h1>
                {{/with}}
                {{#with lead}}
                    <div class="ShowPage-lead">{{this}}</div>
                {{/with}}
                {{#with slug}}
                    <div class="ShowPage-slug">{{this}}</div>
                {{/with}}
                {{#with shortDescription}}
                    <div class="ShowPage-shortDescription">{{this}}</div>
                {{/with}}
                {{#with fullDescription}}
                    <div class="ShowPage-fullDescription" itemprop="description" itemscope itemtype="https://schema.org/description">{{this}}</div>
                {{/with}}
                {{#with genres}}
                    <ul class="ShowPage-genres">
                        {{#each this}}
                            <li class="ShowPage-genres" itemprop="genre" itemscope itemtype="https://schema.org/genre">{{this}}</li>
                        {{/each}}
                    </ul>
                {{/with}}
            </article>
            {{#with tags}}
                <div class="ShowPage-tags">
                    <h2 class="ShowPage-tags-heading">{{format "/core/page/ContentPage" "tagsHeading"}}</h2>
                    <div class="ShowPage-tags-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with countries}}
                <div class="ShowPage-countries">
                    <h2 class="ShowPage-countries-heading">{{format "/core/page/ContentPage" "countriesHeading"}}</h2>
                    <div class="ShowPage-countries-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with brands}}
                <div class="ShowPage-brands">
                    <h2 class="ShowPage-brands-heading">{{format "/core/page/ContentPage" "brandsHeading"}}</h2>
                    <div class="ShowPage-brands-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with products}}
                <div class="ShowPage-products">
                    <h2 class="ShowPage-products-heading">{{format "/core/page/ContentPage" "productsHeading"}}</h2>
                    <div class="ShowPage-products-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with topics}}
                <div class="ShowPage-topics">
                    <h2 class="ShowPage-topics-heading">{{format "/core/page/ContentPage" "topicsHeading"}}</h2>
                    <div class="ShowPage-topics-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with actions}}
                <div class="ShowPage-actions">
                    <h2 class="ShowPage-actions-heading">{{format "/core/page/ContentPage" "actionsHeading"}}</h2>
                    <div class="ShowPage-actions-body">{{this}}</div>
                </div>
            {{/with}}
            {{#with comments}}
                <div class="ShowPage-comments">
                    {{this}}
                </div>
            {{/with}}
        </main>
        {{#with aside}}
            <aside class="ShowPage-aside">{{this}}</aside>
        {{/with}}
        {{#with below}}
            <div class="ShowPage-below">{{this}}</div>
        {{/with}}
        <footer class="ShowPage-footer" itemscope itemtype="https://schema.org/WPFooter">
            {{include "/core/page/Page-logo.amp.hbs" this}}
            {{include "/core/page/Page-navigation.amp.hbs" this}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#with disclaimer}}
                <div class="ShowPage-disclaimer">{{this}}</div>
            {{/with}}
        </footer>
        {{include "/core/page/Page-ampIntegrations.amp.hbs" this}}
        {{include "/core/page/Page-ampAnalytics.amp.hbs" this}}
    </body>
</html>
