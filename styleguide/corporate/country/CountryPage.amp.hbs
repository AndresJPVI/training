<!DOCTYPE html>
<html class="CountryTaxonomyPage" lang="{{#with language}}{{this}}{{else}}en{{/with}}" itemscope itemtype="http://schema.org/WebPage">
    {{include "/core/page/Page-head.hbs" this}}
    <body class="CountryTaxonomyPage-body"{{~#each extraBodyAttributes}} {{this}}{{~/each~}}>
        <header class="CountryTaxonomyPage-header" itemscope itemtype="http://schema.org/WPHeader">
            {{#with hat}}
                <div class="CountryTaxonomyPage-hat">{{this}}</div>
            {{/with}}
            {{#with logo}}
                <div class="CountryTaxonomyPage-logo">{{this}}</div>
            {{/with}}
            {{#with navigation}}
                <div class="CountryTaxonomyPage-navigation">{{this}}</div>
            {{/with}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#if searchAction}}
                <form class="CountryTaxonomyPage-search" action="{{searchAction}}" novalidate="" autocomplete="off">
                    <input type="text" class="CountryTaxonomyPage-searchInput" name="q">
                    <button type="submit" class="CountryTaxonomyPage-searchButton">{{format "/core/page/Page" "searchButtonLabel"}}</button>
                </form>
            {{/if}}
        </header>
        {{#with banner}}
            <div class="CountryTaxonomyPage-banner">{{~this~}}</div>
        {{/with}}
        {{#with sectionNavigation}}
            <div class="CountryTaxonomyPage-sectionNavigation">{{this}}</div>
        {{/with}}
        {{#with packageNavigation}}
            <div class="CountryTaxonomyPage-packageNavigation">{{this}}</div>
        {{/with}}
        {{#with above}}
            <div class="CountryTaxonomyPage-above">{{this}}</div>
        {{/with}}
        <main class="CountryTaxonomyPage-main">
            {{#with pageHeading}}
                <h1 class="CountryTaxonomyPage-pageHeading">{{this}}</h1>
            {{/with}}
            {{#with taxonomySynonyms}}
                <div class="CountryTaxonomyPage-taxonomySynonyms">
                    {{~#each this~}}
                        <span class="CountryTaxonomyPage-taxonomySynonyms-synonym"> {{~this~}}</span>
                        {{~#unless @last}}, {{/unless~}}
                    {{~/each~}}
                </div>
            {{/with}}
            {{#with taxonomyDefinition}}
                <p class="CountryTaxonomyPage-taxonomyDefinition">{{this}}</p>
            {{/with}}
            {{#with fullDescription}}
                <div class="CountryTaxonomyPage-fullDescription" itemprop="description" itemscope itemtype="https://schema.org/description">{{this}}</div>
            {{/with}}
            {{#with pageLead}}
                <div class="CountryTaxonomyPage-pageLead">{{this}}</div>
            {{/with}}
            {{#with taxonomyContent}}
                <section class="CountryTaxonomyPage-taxonomyContent">
                    {{#each this}}
                        {{this}}
                    {{/each}}
                </section>
            {{/with}}
        </main>
        {{#with aside}}
            <aside class="CountryTaxonomyPage-aside">{{this}}</aside>
        {{/with}}
        {{#with below}}
            <div class="CountryTaxonomyPage-below">{{this}}</div>
        {{/with}}
        <footer class="CountryTaxonomyPage-footer" itemscope itemtype="https://schema.org/WPFooter">
            {{#with footerLogo}}
                <div class="CountryTaxonomyPage-footerLogo">{{this}}</div>
            {{/with}}
            {{#with footerNavigation}}
                <div class="CountryTaxonomyPage-footerNavigation">{{this}}</div>
            {{/with}}
            {{#with footerContent}}
                <div class="CountryTaxonomyPage-footerContent">{{this}}</div>
            {{/with}}
            {{include "/core/page/Page-social.hbs" this}}
            {{#with disclaimer}}
                <div class="CountryTaxonomyPage-disclaimer">{{this}}</div>
            {{/with}}
        </footer>
        {{include "/core/page/Page-extraBodyItems.hbs" this}}
    </body>
</html>
